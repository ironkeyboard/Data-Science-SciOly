   "source": [
    "def findMean(li):\n",
    "    # awnser\n",
    "    #li = np.array(li)\n",
    "    #mean = li.mean()\n",
    "    mean = sum(li) / len(li)\n",
    "    return mean"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if findMean(li1) == 3:\n",
