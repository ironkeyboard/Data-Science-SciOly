    "    print(\"2 is wrong\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def findMedian(li):\n",
    "    li.sort()\n",
    "    # awnser\n",
    "    listlength = len(li) \n",
    "    num = listlength//2\n",
    "    if listlength%2==0:\n",
    "        median = (li[num]+li[num-1])/2\n",
    "    else:\n",
    "        median = li[num]\n",
    "    return median"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
