   "outputs": [],
   "source": [
    "def findMin(li):\n",
    "    li.sort()\n",
    "    # awnser\n",
    "    minOfLi = li[0]\n",
    "    return minOfLi"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if findMin(li1) == -2:\n",
