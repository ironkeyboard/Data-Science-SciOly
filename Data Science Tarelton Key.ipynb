    "if findMin(li2) == -5:\n",
    "    print(\"2 is correct!\")\n",
    "else:\n",
    "    print(\"2 is wrong\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def findMax(li):\n",
    "    li.sort()\n",
    "    # awnser\n",
    "    maxOfLi = li[-1]\n",
    "    return maxOfLi"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if findMax(li1) == 10:\n",
