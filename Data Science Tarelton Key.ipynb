    "else:\n",
    "    print(\"1 is wrong\")\n",
    "if findMean(li2) == 4.375:\n",
    "    print(\"2 is correct!\")\n",
    "else:\n",
    "    print(\"2 is wrong\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
